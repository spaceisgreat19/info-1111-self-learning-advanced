{
  "version": 2,
  "routes": [
    {
      "src": "/api/finance",
      "dest": "/api/finance"
    },
    {
      "src": "/api/log-maintenance",
      "dest": "/api/log-maintenance"
    },
    {
      "src": "/api/maintenance",
      "dest": "/api/maintenance"
    },
    {
      "src": "/api/vote",
      "dest": "/api/vote"
    }
  ],
  "regions": ["syd1"],
  "crons": [
    {
      "path": "/api/maintenance-check",
      "schedule": "0 0 * * *"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.example.com"
  }
}
